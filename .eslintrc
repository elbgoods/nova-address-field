{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended",
    "@vue/standard"
  ],
  "globals": {
    "set": true,
    "context": true
  },
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  }
}
